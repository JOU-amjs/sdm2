{
  "name": "string-discontinuous-match",
  "version": "1.1.0",
  "description": "A high performance string discontinuous search function",
  "main": "dist/string-discontinuous-match.js",
  "types": "typings/index.d.ts",
  "module": "dist/string-discontinuous-match.esm.js",
  "unpkg": "dist/string-discontinuous-match.umd.js",
  "jsdelivr": "dist/string-discontinuous-match.umd.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "nyc mocha test/**/*.spec.ts",
    "release": "npm test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags",
    "lint": "eslint -c .eslintrc.js 'src/**/*.ts'",
    "lint:fix": "eslint -c .eslintrc.js 'src/**/*.ts' --fix",
    "build:self": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:umd": "rollup -c config/rollup.config.umd.js",
    "build:umd.min": "cross-env NODE_ENV=production rollup -c config/rollup.config.umd.js",
    "build": "npm run clean && npm run build:self && npm run build:esm && npm run build:umd && npm run build:umd.min",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "generate": "node generate-test-data.js 10000"
  },
  "author": "JOU-amjs",
  "license": "MIT",
  "homepage": "https://github.com/JOU-amjs/string-discontinuous-match#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/JOU-amjs/string-discontinuous-match.git"
  },
  "bugs": {
    "url": "https://github.com/JOU-amjs/string-discontinuous-match/issues"
  },
  "files": [
    "/dist",
    "typings/*.d.ts"
  ],
  "keywords": [
    "string",
    "match",
    "string match",
    "discontinuous match",
    "search",
    "discontinuous search"
  ],
  "devDependencies": {
    "@js-lib/cli": "^1.6.0",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-plugin-istanbul": "5.1.0",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "es5-shim": "4.5.10",
    "eslint": "^5.15.1",
    "expect.js": "0.3.1",
    "mocha": "3.5.3",
    "nyc": "13.1.0",
    "rimraf": "2.6.2",
    "rollup": "^2.10.9",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "source-map-support": "0.5.9",
    "ts-node": "7.0.1",
    "tslib": "1.9.3",
    "typescript": "^3.9.3"
  }
}
